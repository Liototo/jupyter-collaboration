{
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "examples/*",
    "packages/*",
    "tools/*"
  ],
  "scripts": {
    "build": "lerna run build",
    "build:test": "lerna run build:test",
    "build:tsc": "tsc --build tools/* packages/* examples/*",
    "clean": "lerna run clean",
    "clean:all": "lerna run clean:all && rimraf node_modules yarn.lock",
    "clean:tests": "lerna run clean:tests",
    "get:dependency": "get-dependency",
    "publish": "npm run clean && npm run build && lerna publish -m \"Publish\" from-package",
    "remove:dependency": "remove-dependency",
    "test": "lerna run test",
    "test:chrome": "lerna run test:chrome",
    "test:firefox": "lerna run test:firefox",
    "test:firefox:headless": "lerna run test:firefox:headless",
    "test:ie": "lerna run test:ie --concurrency 1",
    "update:dependency": "update-dependency --lerna",
    "version": "lerna version",
    "relay:start": "yarn workspace @jupyterlab-rtc/relay run start",
    "relay:wait": "yarn workspace @jupyterlab-rtc/relay run wait",
    "lumino-example:start": "yarn workspace @jupyterlab-rtc/lumino-example run start",
    "todo-example:start": "yarn workspace @jupyterlab-rtc/todo-example run start",
    "todo-example:wait-then-start": "npm-run-all relay:wait todo-example:start",
    "todo-example:start-all": "npm-run-all --parallel --race relay:start todo-example:wait-then-start",
    "jupyter-supernode:start": "yarn workspace @jupyterlab-rtc/supernode run start",
    "jupyter-supernode:wait-then-start": "npm-run-all relay:wait jupyter-supernode:start",
    "jupyter-server:start": "yarn workspace @jupyterlab-rtc/supernode run start:server",
    "jupyter-example:start": "yarn workspace @jupyterlab-rtc/jupyter-example run start",
    "jupyter-example:wait-then-start": "npm-run-all relay:wait jupyter-example:start",
    "jupyter-example:start-all": "npm-run-all --parallel --race relay:start jupyter-server:start jupyter-example:wait-then-start jupyter-supernode:wait-then-start",
    "lint": "npm-run-all lint:typescript lint:duplicates",
    "lint:duplicates": "yarn run yarn-deduplicate --list --fail",
    "lint:typescript": "yarn run lint:typescript:check --fix",
    "lint:typescript:check": "eslint . --ext 'ts,tsx' --ignore-path .gitignore"
  },
  "dependencies": {},
  "resolutions": {
    "lodash": "4.17.15",
    "rxjs": "6.6.0"
  },
  "devDependencies": {
    "@jupyterlab/buildutils": "^2.0.0-rc.0",
    "@typescript-eslint/eslint-plugin": "3.6.0",
    "@typescript-eslint/parser": "3.6.0",
    "create-react-app": "3.4.1",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-react": "7.20.3",
    "eslint-plugin-react-hooks": "4.0.6",
    "lerna": "^3.19.0",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "yarn-deduplicate": "2.0.0"
  }
}
